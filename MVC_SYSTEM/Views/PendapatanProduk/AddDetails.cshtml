    int feb = 15;
    int mac = 20;
    int apr = 25;
    int may = 30;
    int jun = 35;
    int jul = 40;
    int aug = 45;
    int sep = 50;
    int oct = 55;
    int nov = 60;
    int dec = 65;
    int qty_totalb = 70;
    int qty_grandtotal = 75;
