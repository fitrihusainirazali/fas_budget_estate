        $('input[type="number"]').on('keydown', function (e) {
            if ($(this).val() < 0 && e.keyCode !== 8 && e.keyCode !== 46) {
                e.preventDefault();
            }
            else if (e.key === "-" || e.key === "+" || e.key === "e" || e.key === "E") {
                e.preventDefault();
            }
        });

        $('.decimal').keyup(function () {
            var val = $(this).val();
            if (isNaN(val)) {
                val = val.replace(/[^0-9\.]/g, '');
                if (val.split('.').length > 2)
                    val = val.replace(/\.+$/, "");
            }
            $(this).val(val);
        });
