        @Html.Hidden("bgtlocation", null, new { @class = "form-control", @readonly = "true" });
        @Html.AntiForgeryToken()
        <div class="modal-body">
            <div class="form-horizontal">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="panel panel-success">
                    <div class="panel-heading">@GetBudgetClass.GetScreenName("I1")</div>
                    <div class="panel-body">
                        <div class="form-group">
                            <div class="col-sm-4">
                                <label>Tahun</label>
                                @Html.TextBox("budgetyear", budgetyear, new { @class = "form-control", @readonly = "readonly" })
                                @Html.ValidationMessageFor(m => m[i].abio_budgeting_year, "", new { @class = "text-danger" })
                            </div>
                            <div class="col-sm-4">
                                <label>Produk</label>
                                @Html.DropDownList("product_code", new SelectList(ViewBag.ProductList, "Value", "Text"), htmlAttributes: new { @class = "form-control" })
                                @Html.ValidationMessageFor(m => m[i].abio_product_name, "", new { @class = "text-danger" })
                                @Html.ValidationMessage("ProductList", "", new { @class = "text-danger" })
                            </div>
                            <div class="col-sm-4">
                                &nbsp;
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4">
                                <label>@GlobalResEstate.lblCostCenter</label>
                                @Html.DropDownList("abiocostcenter", new SelectList(ViewBag.CostCenterList, "Value", "Text"), "Please Select", htmlAttributes: new { @class = "CostCenter form-control select2", @required = "true" })
