<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.select2').select2({
            width: '100%'
        });

        //glChange();
        assetChange();
        jenisChange();

        setTotal();
        prorationChange();
        setBalanceQty();
        setTotalQty();
        setTotalAmount();

        $('input[type="number"]').on('keydown', function (e) {
            var val = $(this).val();
            //var regex = /^\d+(\.\d{0,1})?$/;
            var regex = /^\d+(\.\d{0,2})?$/;
            if (!regex.test(val)) {
                var match = val.match(regex);
                if (match !== null) {
                    $(this).val(match[0]);
                } else {
                    var newVal = val.replace(/(\.\d{1})\d+/g, '$1');
                    $(this).val(newVal);
                }
            }
            if ($(this).val() < 0 && e.keyCode !== 8 && e.keyCode !== 46) {
                e.preventDefault();
            } else if (e.key === '-' || e.key === '+' || e.key === 'e' || e.key === 'E') {
                e.preventDefault();
            }
        });

        $('.input-number').on('focus', function () {
            $(this).val($(this).val().replace(/,/g, ''));
            $(this).attr('type', 'number');
        });
        $('.input-number').on('blur', function () {
            $(this).attr('type', 'text');
            if ($(this).val()) {
                var value = parseFloat($(this).val());
                $(this).val(value.toLocaleString(undefined, {
                    minimumFractionDigits: 2,
                    maximumFractionDigits: 2
                }));
            }
        });

        $('.input-number-qty').on('focus', function () {
            $(this).val($(this).val().replace(/,/g, ''));
            $(this).attr('type', 'number');
        });
        $('.input-number-qty').on('blur', function () {
            $(this).attr('type', 'text');
            if ($(this).val()) {
                var value = parseFloat($(this).val());
                $(this).val(value.toLocaleString(undefined, {
                    minimumFractionDigits: 3,
                    maximumFractionDigits: 3
                }));
            }
        });

        $('.input_amount_proration').attr('readonly', 'readonly');

        //$('#abce_gl_code').on('change', function () {
        //    glChange();
        //});

        $('#abce_ast_code').on('change', function () {
            assetChange();
        });

        $('#abce_jenis_code').on('change', function () {
            jenisChange();
        });

        $('#abce_proration').on('change', function () {
            prorationChange(true);
            setBalanceQty();
            setTotalQty();
            setTotalAmount();
        });

        $('#abce_quantity_1, #abce_quantity_2, #abce_quantity_3, #abce_quantity_4, #abce_quantity_5, #abce_quantity_6, #abce_quantity_7, #abce_quantity_8, #abce_quantity_9, #abce_quantity_10, #abce_quantity_11, #abce_quantity_12').on('keyup', function () {
            setTotalQty();
            setBalanceQty();
        });

        $('#abce_quantity_1').on('keyup', function () {
            setAmount1();
        });

        $('#abce_quantity_2').on('keyup', function () {
            setAmount2();
        });

        $('#abce_quantity_3').on('keyup', function () {
            setAmount3();
        });

        $('#abce_quantity_4').on('keyup', function () {
            setAmount4();
        });

        $('#abce_quantity_5').on('keyup', function () {
            setAmount5();
        });

        $('#abce_quantity_6').on('keyup', function () {
            setAmount6();
        });

        $('#abce_quantity_7').on('keyup', function () {
            setAmount7();
        });

        $('#abce_quantity_8').on('keyup', function () {
            setAmount8();
        });

        $('#abce_quantity_9').on('keyup', function () {
            setAmount9();
        });

        $('#abce_quantity_10').on('keyup', function () {
            setAmount10();
        });

        $('#abce_quantity_11').on('keyup', function () {
            setAmount11();
        });

        $('#abce_quantity_12').on('keyup', function () {
            setAmount12();
        });

        $('#abce_unit_1, #abce_rate').on('keyup', function () {
            setTotal();
            prorationChange(true);
            setBalanceQty();
            setTotalQty();
            setTotalAmount();
        });

        $('#abce_unit_1, #abce_rate').on('change', function () {
            setTimeout(function () {
                setTotal();
                prorationChange(true);
                setBalanceQty();
                setTotalQty();
                setTotalAmount();
            }, 500);
        });
    });

    function assetChange() {
        var val = $('#abce_ast_code').find('option:selected').val();
        if (val) {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetAssetCategoryByID")',
                dataType: 'json',
                contentType: 'application/json',
                async: false,
                data: JSON.stringify({ id: val }),
                success: function (result) {
                    $('#abce_ast_category').val(result.assetCategory.ast_Category);
                    updateGlByCode(result.assetCategory.ast_GL);
                }
            });
        }
    }

    function updateGlByCode(code) {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetGlByCode")',
            dataType: 'json',
            contentType: 'application/json',
            async: false,
            data: JSON.stringify({ code: code }),
            success: function (result) {
                $('#asset_gl').val(result.gl.Code + ' - ' + result.gl.Description);
                $('#abce_gl_code').val(result.gl.Code);
                $('#abce_gl_name').val(result.gl.Description);
            }
        });
    }

    function glChange() {
        var val = $('#abce_gl_code').find('option:selected').val();
        var text = $('#abce_gl_code').find('option:selected').text();
        if (val) {
            $('#abce_gl_name').val(text.substr(13));
        } else {
            $('#abce_gl_name').val(null);
        }
    }

    function jenisChange() {
        var val = $('#abce_jenis_code').find('option:selected').val();
        var text = $('#abce_jenis_code').find('option:selected').text();
        if (val) {
            $('#abce_jenis_name').val(text);
        } else {
            $('#abce_jenis_name').val(null);
        }
    }

    function prorationChange(trigger = false) {
        var val = $('#abce_proration').find('option:selected').val();
        if (val) {
            if (val == 'keyin') {
                $('.input_proration').removeAttr('readonly');

                if (trigger) {
                    $('#abce_quantity_1').val(null);
                    $('#abce_quantity_2').val(null);
                    $('#abce_quantity_3').val(null);
                    $('#abce_quantity_4').val(null);
                    $('#abce_quantity_5').val(null);
                    $('#abce_quantity_6').val(null);
                    $('#abce_quantity_7').val(null);
                    $('#abce_quantity_8').val(null);
                    $('#abce_quantity_9').val(null);
                    $('#abce_quantity_10').val(null);
                    $('#abce_quantity_11').val(null);
                    $('#abce_quantity_12').val(null);

                    $('#abce_amount_1').val(null);
                    $('#abce_amount_2').val(null);
                    $('#abce_amount_3').val(null);
                    $('#abce_amount_4').val(null);
                    $('#abce_amount_5').val(null);
                    $('#abce_amount_6').val(null);
                    $('#abce_amount_7').val(null);
                    $('#abce_amount_8').val(null);
                    $('#abce_amount_9').val(null);
                    $('#abce_amount_10').val(null);
                    $('#abce_amount_11').val(null);
                    $('#abce_amount_12').val(null);
                }
            } else {
                $('.input_proration').attr('readonly', 'readonly');

                var qty = $('#abce_unit_1').val() ? parseFloat($('#abce_unit_1').val().replace(/,/g, '')) : 0;
                var qty1 = 0;
                var qty2 = 0;
                var qty3 = 0;
                var qty4 = 0;
                var qty5 = 0;
                var qty6 = 0;
                var qty7 = 0;
                var qty8 = 0;
                var qty9 = 0;
                var qty10 = 0;
                var qty11 = 0;
                var qty12 = 0;

                if (val == 'monthly') {
                    qty1 = qty / 12;
                    qty2 = qty / 12;
                    qty3 = qty / 12;
                    qty4 = qty / 12;
                    qty5 = qty / 12;
                    qty6 = qty / 12;
                    qty7 = qty / 12;
                    qty8 = qty / 12;
                    qty9 = qty / 12;
                    qty10 = qty / 12;
                    qty11 = qty / 12;
                    qty12 = qty - (parseFloat((qty / 12).toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }).replace(/,/g, '')) * 11);
                } else if (val == 'quarterly') {
                    qty3 = qty / 4;
                    qty6 = qty / 4;
                    qty9 = qty / 4;
                    qty12 = qty - (parseFloat((qty / 4).toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }).replace(/,/g, '')) * 3);
                } else if (val == 'semi_annually') {
                    qty6 = qty / 2;
                    qty12 = qty - parseFloat(qty6.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }).replace(/,/g, ''));
                } else if (val == 'annually') {
                    qty12 = qty;
                }

                $('#abce_quantity_1').val(qty1.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_2').val(qty2.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_3').val(qty3.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_4').val(qty4.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_5').val(qty5.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_6').val(qty6.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_7').val(qty7.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_8').val(qty8.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_9').val(qty9.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_10').val(qty10.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_11').val(qty11.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));
                $('#abce_quantity_12').val(qty12.toLocaleString(undefined, { minimumFractionDigits: 3, maximumFractionDigits: 3 }));

                setAmount1();
                setAmount2();
                setAmount3();
                setAmount4();
                setAmount5();
                setAmount6();
                setAmount7();
                setAmount8();
                setAmount9();
                setAmount10();
                setAmount11();
                setAmount12();
            }
        }
    }

    function setTotal() {
        var total = calcTotal();
        $('#total_amount').val(total.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2
        }));
    }

